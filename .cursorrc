{
  "ai": {
    "autocomplete": {
      "enabled": true,
      "delay": 0
    },
    "autorun": {
      "enabled": true
    },
    "chat": {
      "enabled": true,
      "previewChanges": true
    },
    "inline": {
      "enabled": true
    }
  },
  "editor": {
    "formatOnSave": true,
    "tabSize": 2,
    "wordWrap": "on"
  },
  "extensions": {
    "recommendations": [
      "esbenp.prettier-vscode",
      "dbaeumer.vscode-eslint",
      "bradlc.vscode-tailwindcss",
      "dsznajder.es7-react-js-snippets"
    ]
  }
} 